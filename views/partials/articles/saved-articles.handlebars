<div id="news">
    <a class="current-link" href="{{link}}">
        <h3 class="current-title">{{title}}</h3>
        <img src="{{image}}" class="current-Image">
    </a>

    <p class="current-date">{{date}}</p>
    <a class="btn btn-primary btn-lg remove-article-btn" data-id="{{_id}}" role="button">Remove</a>
    <a class="btn btn-primary btn-lg note-article-btn" data-id="{{_id}}" role="button">Note</a>

</div>